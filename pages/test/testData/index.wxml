<view class="details-wrap">
  <view class="fixedArea">
    <button class="rs-btn testDataEnter" plain="true" bindtap="toAddTestData">新增试验数据</button>
  </view>
  <view class="main mt-20">
    <scroll-view class="main-wrapper" scroll-y style="height:100%">
      <block wx:if="{{pileList.length>0}}">
        <view class="plie-items" bindtap="" wx:for="{{pileList}}" wx:key="{{index}}" data-id="{{item.baseInfoId}}">
          <view class="pile-order">{{index+1}}</view>
          <view class="pile-no">{{item.pileNo}}</view>
        </view>
      </block>
      <view wx:else style="color:#fff;">暂无桩号</view>
    </scroll-view>
  </view>
  <view class="footer backfooter">
    <button class="rs-btn backbtn" plain="true" bindtap="toWtDetails">返回</button>
  </view>
</view>