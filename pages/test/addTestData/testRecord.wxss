/* pages/test/TestDataDetails/TestDataDetails.wxss */
.details-container.pb-10{
  padding-bottom:10rpx;
}
.btn-sure {
  flex: 0 0 240rpx;
  display: inline-block;
  vertical-align: middle;
}
.rs-xs-btn {
  width: 200rpx;
  padding: 22rpx 0;
  line-height: unset;
}
.rs-xs-btn.btn-green {
  color: #4cd964;
  border: 1px solid #4cd964;
}
.rs-btn.testDataEnter {
  float: right;
  margin-right: 100rpx;
  margin-top: 20rpx;
}
.weui-cell::before {
  border: none;
}
.weui-cell.weui-cell_access {
  padding:0 30rpx 0 0;
}

.depthNum {
  flex: 0 0 100rpx;
  margin-right:100rpx;
  line-height:100rpx;
  text-align: center;
  background-color: rgb(223, 223, 223);
}

.testNum {
  display: inline-block;
  vertical-align: middle;
  color: #4cd964;
  margin-right: 30rpx;
}

.weui-cell_active {
  background-color: #383838;
}

.weui-textarea, .weui-input {
  color: #4cd964;
}
.weui-textarea{
  padding-top:18rpx;
}
